Host *
  User toshi
  IdentityFile ~/.ssh/id_rsa
  ForwardAgent yes
  ServerAliveInterval 30
  ServerAliveCountMax 5
  GSSAPIAuthentication no
  StrictHostKeyChecking no
  UserKnownHostsFile=/dev/null

Host github.com
  HostName github.com
  AddKeysToAgent yes
  User git
  IdentityFile ~/.ssh/gitkey

Host gitlab.com
  HostName gitlab.com
  AddKeysToAgent yes
  User git
  IdentityFile ~/.ssh/gitkey
